package com.sonatatwo;

public class SimpleExample {
	
	int a =10;
	int b =15;
	
	private int add(int a , int b) {
		return a+b;
	}
	
	int sub(int a ,int b) {
		return b-a;
	}
	
	protected int mul(int a ,int b) {
		return a*b;
		
	}
  public int div(int a ,int b) {
	  return a/b;
	  
  }
   public static void main(String[] args) {
}
}
