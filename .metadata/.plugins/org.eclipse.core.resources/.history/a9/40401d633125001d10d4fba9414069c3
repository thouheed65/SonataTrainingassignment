package sonata.Lambdaexample;

public class Book {
	private int id;
	private String name;
	private int page;
	public Book(int id , String name, int page) {
		this.id = id;
		this.name = name;
		this.page = pagee;
		
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPage() {
		return page;
	}
	public void setPage(int page) {
		this.page = page;
	}
	@Override
	public String toString() {
		return "Book [id=" + id + ", name=" + name + ", page=" + page + "]";
	}
	
	

}
